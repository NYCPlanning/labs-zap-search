{
  "name": "zap-api-nest-test",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "engines": {
    "node": "^10.15.3"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:dev:skip-auth": "SKIP_AUTH=true yarn run start:dev",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js",
    "copy-query-files": "cp -R src/project/_queries dist/project/",
    "postinstall": "yarn run build",
    "migrate": "node-pg-migrate"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/typeorm": "^6.1.3",
    "@octokit/rest": "^16.43.1",
    "@turf/bbox": "^6.0.1",
    "@turf/buffer": "^5.1.5",
    "@turf/helpers": "^6.1.4",
    "@turf/turf": "^5.1.6",
    "@types/dotenv": "^6.1.1",
    "adal-node": "^0.2.1",
    "app-root-path": "^3.0.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.1.0",
    "eol": "^0.9.1",
    "express-recaptcha": "^5.0.1",
    "inflected": "^2.0.4",
    "json2csv": "^4.5.4",
    "jsonapi-serializer": "^3.6.5",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "nest-schedule": "^0.6.3",
    "nestjs-typeorm-paginate": "^1.0.2",
    "nock": "^11.7.0",
    "node-cache": "^4.2.1",
    "node-pg-migrate": "^3.23.3",
    "odata": "^1.0.4",
    "odata-parser": "^1.4.1",
    "odata-v4-parser": "^0.1.29",
    "p-limit": "^2.2.1",
    "pg": "^7.12.1",
    "pg-promise": "^9.2.1",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "shortid": "^2.2.15",
    "sphericalmercator": "^1.0.5",
    "superagent": "^5.2.2",
    "ts-loader": "^6.1.2",
    "typeorm": "^0.2.19",
    "underscore": "^1.9.1",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-node-externals": "^1.7.2",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.11.2",
    "@nestjs/testing": "^6.0.0",
    "@types/express": "^4.17.1",
    "@types/jest": "24.0.11",
    "@types/jsonwebtoken": "^8.3.4",
    "@types/node": "11.13.4",
    "@types/supertest": "2.0.7",
    "jest": "24.7.1",
    "prettier": "1.17.0",
    "supertest": "5.0.0-0",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
